struct Node {
    int data;
    Node *next;
    Node *random;

    Node(int x) {
        data = x;
        next = NULL;
        random = NULL;
    }
};*/

class Solution {
  public:
    Node *copyList(Node *head) {
        // Write your code 
        if(head==nullptr){
            return nullptr;
        }
        return head;
    }
};
